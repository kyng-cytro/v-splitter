{
  "compilerOptions": {
    "lib": ["es2023"],
    "outDir": "bin",
    "module": "Node16",
    "target": "es2022",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16"
  },
  "include": ["src"]
}
